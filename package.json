{
  "name": "vue-google-charts",
  "description": "Reactive Vue.js wrapper for Google Charts lib",
  "version": "0.3.2",
  "author": {
    "name": "Refat",
    "email": "nobilix@gmail.com"
  },
  "keywords": [
    "vue",
    "vuejs",
    "charts",
    "google charts",
    "charts component"
  ],
  "license": "MIT",
  "main": "dist/vue-google-charts.common.js",
  "unpkg": "dist/vue-google-charts.browser.js",
  "module": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --config config/webpack.config.dev.js --progress --watch",
    "build": "npm run build:browser && npm run build:common",
    "lint": "eslint --ext .js,.vue src --fix",
    "build:browser": "cross-env NODE_ENV=production webpack --config config/webpack.config.browser.js --progress --hide-modules",
    "build:common": "cross-env NODE_ENV=production webpack --config config/webpack.config.common.js --progress --hide-modules",
    "prepublishOnly": "yarn build",
    "commit": "cz",
    "enableGitHooks": "simple-git-hooks",
    "cleanPublish": "clean-publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devstark-com/vue-google-charts.git"
  },
  "bugs": {
    "url": "https://github.com/devstark-com/vue-google-charts/issues"
  },
  "homepage": "https://github.com/devstark-com/vue-google-charts#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.0.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@commitlint/cz-commitlint": "^16.0.0",
    "babel-core": "^6.0.0",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.16.0",
    "clean-publish": "^3.4.5",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "nano-staged": "^0.5.0",
    "simple-git-hooks": "^2.7.0",
    "vue-loader": "^10.0.0",
    "vue-template-compiler": "^2.1.6",
    "webpack": "^2.1.0-beta.28",
    "webpack-merge": "^1.1.2"
  },
  "dependencies": {
    "debounce": "^1.1.0"
  }
}
